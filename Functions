//JS Page 

console.log("This is about functions");

//Create function
function mySubjects(subject,message){
console.log(subject+" : " +message);
}

function myFunction(a,b,c){
     let sum = a+b+c;
     return sum;    
}

let subj = "CN";
let subj2 = "DBMS";;
let subj3 = "SE";
let subj4 = "python";
let subj5 = "web Development";
let msg = "This is your subject";

//call function
mySubjects(subj,msg);//known as arguments
mySubjects(subj2,msg);
mySubjects(subj3,msg);
mySubjects(subj4,msg);
mySubjects(subj5,msg);

// console.log(subj +": This is your subject.");
