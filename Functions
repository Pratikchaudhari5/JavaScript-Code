// console.log(subj3 +": This is your subject.");
// console.log(subj4 +": This is your subject.");
// console.log(subj5 +": This is your subject.");
//to reduce this much line of code we use function

//Returning a value
let returnVal = myFunction(1,2,3);
console.log(returnVal);
